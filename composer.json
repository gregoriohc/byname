{
    "name": "gregoriohc/byname",
    "type": "library",
    "description": "PHP trait for bynaming (nicknaming) classes/objects",
    "keywords": [
        "gregoriohc",
        "byname"
    ],
    "homepage": "https://github.com/gregoriohc/byname",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregorio HernÃ¡ndez Caso",
            "email": "gregoriohc@gmail.com",
            "homepage": "https://github.com/gregoriohc",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0|~8.0",
        "gregoriohc/static-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=4.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Gregoriohc\\Byname\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gregoriohc\\Byname\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
